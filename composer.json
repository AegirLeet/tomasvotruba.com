{
    "name": "tomasvotruba/website",
    "require": {
        "php": "^7.1",
        "symplify/statie": "^2.5",
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "J7mbo/twitter-api-php": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\Website\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomasVotuba\\Website\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^2.5",
        "phpstan/phpstan": "^0.8",
        "phpunit/phpunit": "^6.4"
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check bin src tests",
        "fix-cs": "vendor/bin/ecs check bin src tests --fix",
        "phpstan": "vendor/bin/phpstan analyse bin src tests --level max"
    }
}
