{
    "name": "tomasvotruba/website",
    "license": "MIT",
    "description": "Blog about hacking PHP, education and being the laziest PHP programmer in the world",
    "require": {
        "php": "^7.4",
        "doctrine/cache": "^1.10",
        "erusev/parsedown-extra": "^0.8.1",
        "nette/finder": "^2.5",
        "nette/utils": "^3.0",
        "symfony/asset": "^5.0",
        "symfony/cache": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.6",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-foundation": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/twig-bridge": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symplify/auto-bind-parameter": "^7.3",
        "symplify/autodiscovery": "^7.3",
        "symplify/autowire-array-parameter": "^7.3",
        "symplify/flex-loader": "^7.3",
        "symplify/package-builder": "^7.3",
        "symplify/symfony-static-dumper": "^7.3",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "migrify/latte-to-twig": "dev-master",
        "nette/neon": "^3.1",
        "phpstan/phpstan": "^0.12.14",
        "phpunit/phpunit": "^9.0",
        "rector/rector": "dev-master",
        "symfony/console": "^5.0",
        "symfony/process": "^5.0",
        "symfony/stopwatch": "^5.0",
        "symfony/yaml": "^5.0",
        "symplify/easy-coding-standard": "^7.3",
        "symplify/phpstan-extensions": "^7.3",
        "tracy/tracy": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\Website\\": "src",
            "TomasVotruba\\FrameworkStats\\": "packages/framework-stats/src",
            "TomasVotruba\\SymfonyStaticDump\\": "packages/symfony-static-dump/src",
            "TomasVotruba\\Blog\\": "packages/blog/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomasVotruba\\Website\\Tests\\": "tests",
            "TomasVotruba\\FrameworkStats\\Tests\\": "packages/framework-stats/tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse bin src tests packages --ansi --error-format symplify",
        "check-cs": "vendor/bin/ecs check bin src tests packages --ansi",
        "fix-cs": [
            "vendor/bin/ecs check bin src tests packages --fix --ansi",
            "bin/clear_posts_trailing_whitespaces.sh"
        ],
        "check-status": "vendor/bin/http-status-check scan https://tomasvotruba.com --output invalid-links.txt --ansi",
        "rector": "vendor/bin/rector process src tests packages --dry-run --config rector-ci.yaml --ansi",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
