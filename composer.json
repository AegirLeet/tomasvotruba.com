{
    "require": {
        "php": ">=5.6",
        "beberlei/assert": "~2.3",
        "doctrine/doctrine-bundle" : "~1.5",
        "doctrine/orm": "~2.5",
        "erusev/parsedown": "~1.5",

        "ext-pdo_sqlite"                       : "*",
        "incenteev/composer-parameter-handler" : "~2.1",
        "leafo/scssphp"                        : "~0.1.5",
        "patchwork/jsqueeze"                   : "~1.0",
        "symfony/symfony"                      : "~2.7",
        "twig/extensions"                      : "~1.2",

        "sensio/distribution-bundle": "dev-symfony-3 as 3.0",
        "sensio/framework-extra-bundle" : "~3.0",

        "symfony/assetic-bundle" : "dev-symfony-3 as 2.6",
        "symfony/framework-bundle": "dev-master as 2.7",
        "symfony/debug-bundle": "dev-master as 3.0",
        "symfony/security-bundle": "dev-master as 3.0",
        "symfony/twig-bundle": "dev-master as 3.0",
        "symfony/twig-bridge": "dev-master as 3.0",
        "symfony/web-profiler-bundle": "dev-master as 3.0",

        "symfony/asset": "dev-master as 3.0",
        "symfony/config": "dev-master as 3.0",
        "symfony/debug" : "dev-master as 3.0",
        "symfony/class-loader": "dev-master as 3.0",
        "symfony/dependency-injection": "dev-master as 3.0",
        "symfony/event-dispatcher": "dev-master as 3.0",
        "symfony/filesystem": "dev-master as 3.0",
        "symfony/form" : "dev-master as 3.0",
        "symfony/http-foundation" : "dev-master as 3.0",
        "symfony/http-kernel" : "dev-master as 3.0",
        "symfony/security-acl" : "dev-master as 3.0",
        "symfony/security-core" : "dev-master as 3.0",
        "symfony/security" : "dev-master as 2.6",
        "symfony/security-csrf" : "dev-master as 3.0",
        "symfony/routing": "dev-master as 3.0",
        "symfony/stopwatch": "dev-master as 3.0",
        "symfony/serializer": "dev-master as 3.0",
        "symfony/templating": "dev-master as 3.0",
        "symfony/translation": "dev-master as 3.0",
        "symfony/var-dumper": "dev-master as 3.0",
        "symfony/validator": "dev-master as 3.0",
        "symfony/intl": "dev-master as 3.0",
        "symfony/finder": "dev-master as 3.0",
        "symfony/options-resolver": "dev-master as 3.0",
        "symfony/property-access": "dev-master as 3.0",

        "symnedi/autowiring-bundle": "~0.0.2",
        "ramsey/uuid": "~2.8"
    },
    "repositories": [
        { "type": "vcs", "url": "http://github.com/TomasVotruba/AsseticBundle" },
        { "type": "vcs", "url": "http://github.com/TomasVotruba/SensioDistributionBundle" }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "AppBundle\\": "src/AppBundle",
            "Project\\": "src/Project",
            "Interoperability\\": "src/Interoperability",
            "InteroperabilityAdapter\\": "src/InteroperabilityAdapter",
            "BlogDomain\\": "src/BlogDomain",
            "BlogDomainBundle\\": "src/BlogDomainBundle"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
